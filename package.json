{
  "name": "builder",
  "displayName": "Lancelot Builder (LBR)",
  "description": "Automated build tool for Lancelot",
  "publisher": "roland-strauss",
  "version": "0.6.13",
  "icon": "assets/img/ext/logo.png",
  "keywords": [
    "ibm i",
    "ibmi",
    "rpgle",
    "ile",
    "build"
  ],
  "author": {
    "name": "Roland Strauss",
    "url": "https://www.momentum.co.za"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/rolandstrauss/lbr-extension"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lbr.show_changes",
        "title": "%lbr.show_changes%",
        "icon": "$(check-all)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.show_single_changes",
        "title": "%lbr.show_single_changes%",
        "icon": "$(check)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.run_build",
        "title": "%lbr.run_build%",
        "icon": "$(run-all)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.run_single_build",
        "title": "%lbr.run_single_build%",
        "icon": "$(run)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.update",
        "title": "%lbr.source-filter.update%",
        "icon": "$(refresh)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.add",
        "title": "%lbr.source-filter.add%",
        "icon": "$(add)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.get-remote-compiled-object-list",
        "title": "%lbr.get-remote-compiled-object-list%",
        "icon": "$(arrow-circle-down)",
        "when": "lbr.contains_lbr_project && lbr.valid-config && false"
      },
      {
        "command": "lbr.copy-profile-config",
        "title": "%lbr.copy-profile-config%",
        "icon": "$(person-add)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.reset-compiled-object-list",
        "title": "%lbr.reset-compiled-object-list%",
        "icon": "$(extensions)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.get-remote-source-list",
        "title": "%lbr.get-remote-source-list%",
        "icon": "$(arrow-down)",
        "when": "lbr.contains_lbr_project && lbr.valid-config && false"
      },
      {
        "command": "lbr.check-remote-sources",
        "title": "%lbr.check-remote-sources%",
        "icon": "$(tasklist)",
        "when": "lbr.contains_lbr_project && lbr.valid-config && false"
      },
      {
        "command": "lbr.controller.config",
        "title": "%lbr.controller.config%",
        "icon": "$(settings-gear)",
        "when": "lbr.contains_lbr_project"
      },
      {
        "command": "lbr.controller.dependency-list",
        "title": "%lbr.controller.dependency-list%",
        "icon": "$(outline-view-icon)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source.edit-compile-config",
        "title": "%lbr.source.edit-compile-config%",
        "icon": "$(settings-gear)",
        "when": "lbr.contains_lbr_project"
      },
      {
        "command": "lbr.transfer-all",
        "title": "%lbr.transfer-all%",
        "icon": "$(cloud-upload)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.show-view",
        "title": "%lbr.source-filter.show-view%",
        "icon": "$(preview)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.edit-config",
        "title": "%lbr.source-filter.edit-config%",
        "icon": "$(settings-gear)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.delete-config",
        "title": "%lbr.source-filter.delete-config%",
        "icon": "$(trash)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.view-source-infos",
        "title": "%lbr.source-filter.view-source-infos%",
        "icon": "$(preview)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.maintain-source-infos",
        "title": "%lbr.source-filter.maintain-source-infos%",
        "icon": "$(settings-gear)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.add-source-file",
        "title": "%lbr.source-filter.add-source-file%",
        "icon": "$(add)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.add-source-member",
        "title": "%lbr.source-filter.add-source-member%",
        "icon": "$(add)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.change-source-description",
        "title": "%lbr.source-filter.change-source-description%",
        "icon": "$(preview)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.rename-source-member",
        "title": "%lbr.source-filter.rename-source-member%",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source-filter.delete-source-member",
        "title": "%lbr.source-filter.delete-source-member%",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.source.maintain-source-dependency",
        "title": "%lbr.source.maintain-source-dependency%",
        "icon": "$(combine)",
        "when": "lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.deployment.maintain",
        "title": "%lbr.deployment.maintain%",
        "icon": "$(settings-gear)",
        "when": "1==2 && lbr.contains_lbr_project && lbr.valid-config"
      },
      {
        "command": "lbr.build-history.update",
        "title": "Update Build History"
      },
      {
        "command": "lbr.build-history.delete-item",
        "title": "Delete Build History Item",
        "icon": "$(trash)"
      },
      {
        "command": "lbr.build-history.delete-date",
        "title": "Delete Build History for Date",
        "icon": "$(trash)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "lbr.source-filter.add",
          "when": "lbr.contains_lbr_project && lbr.valid-config && view == lbr.source-filter",
          "group": "navigation@1"
        },
        {
          "command": "lbr.source-filter.view-source-infos",
          "when": "lbr.contains_lbr_project && lbr.valid-config && view == lbr.source-filter",
          "group": "navigation@2"
        },
        {
          "command": "lbr.source-filter.maintain-source-infos",
          "when": "lbr.contains_lbr_project && lbr.valid-config && view == lbr.source-filter",
          "group": "navigation@3"
        },
        {
          "command": "lbr.source-filter.update",
          "when": "lbr.contains_lbr_project && lbr.valid-config && view == lbr.source-filter",
          "group": "navigation@5"
        },
        {
          "command": "lbr.get-remote-source-list",
          "when": "false && lbr.contains_lbr_project && (view == lbr.controller || (!lbr.valid-config && view == lbr-config-invalid))",
          "group": "navigation@20"
        },
        {
          "command": "lbr.get-remote-compiled-object-list",
          "when": "false && lbr.contains_lbr_project && (view == lbr.controller || (!lbr.valid-config && view == lbr-config-invalid))",
          "group": "navigation@30"
        },
        {
          "command": "lbr.reset-compiled-object-list",
          "when": "lbr.contains_lbr_project && (view == lbr.controller || (!lbr.valid-config && view == lbr-config-invalid))",
          "group": "navigation@40"
        },
        {
          "command": "lbr.check-remote-sources",
          "when": "false && lbr.contains_lbr_project && (view == lbr.controller || (!lbr.valid-config && view == lbr-config-invalid))",
          "group": "navigation@50"
        },
        {
          "command": "lbr.transfer-all",
          "when": "lbr.contains_lbr_project && (view == lbr.controller || (!lbr.valid-config && view == lbr-config-invalid))",
          "group": "navigation@60"
        },
        {
          "command": "lbr.controller.dependency-list",
          "when": "lbr.contains_lbr_project && (view == lbr.controller || (!lbr.valid-config && view == lbr-config-invalid))",
          "group": "navigation@65"
        },
        {
          "command": "lbr.controller.config",
          "when": "lbr.contains_lbr_project && (view == lbr.controller || (!lbr.valid-config && view == lbr-config-invalid))",
          "group": "navigation@70"
        },
        {
          "command": "lbr.deployment.maintain",
          "when": "lbr.contains_lbr_project && lbr.valid-config && view == lbr.deployment",
          "group": "navigation@1"
        }
      ],
      "editor/title/context": [
        {
          "command": "lbr.source.edit-compile-config",
          "when": "lbr.contains_lbr_project"
        },
        {
          "command": "lbr.source-filter.change-source-description",
          "when": "lbr.contains_lbr_project"
        },
        {
          "command": "lbr.source.maintain-source-dependency",
          "when": "lbr.contains_lbr_project"
        }
      ],
      "explorer/context": [
        {
          "command": "lbr.source.edit-compile-config",
          "when": "lbr.contains_lbr_project"
        },
        {
          "command": "lbr.source-filter.change-source-description",
          "when": "lbr.contains_lbr_project"
        },
        {
          "command": "lbr.source.maintain-source-dependency",
          "when": "lbr.contains_lbr_project"
        }
      ],
      "view/item/context": [
        {
          "command": "lbr.source-filter.show-view",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == source-list",
          "group": "inline@0"
        },
        {
          "command": "lbr.source-filter.edit-config",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == source-list",
          "group": "inline@2"
        },
        {
          "command": "lbr.source-filter.delete-config",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == source-list",
          "group": "inline@3"
        },
        {
          "command": "lbr.source.edit-compile-config",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == lbr-source",
          "group": "inline@0"
        },
        {
          "command": "lbr.source.maintain-source-dependency",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == lbr-source",
          "group": "inline@1"
        },
        {
          "command": "lbr.source-filter.change-source-description",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == lbr-source",
          "group": "inline@1"
        },
        {
          "command": "lbr.source-filter.rename-source-member",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == lbr-source"
        },
        {
          "command": "lbr.source-filter.delete-source-member",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == lbr-source"
        },
        {
          "command": "lbr.source-filter.add-source-member",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == source-file",
          "group": "inline@0"
        },
        {
          "command": "lbr.source-filter.add-source-file",
          "when": "lbr.contains_lbr_project && view == lbr.source-filter && viewItem == source-lib",
          "group": "inline@0"
        },
        {
          "command": "lbr.build-history.delete-item",
          "when": "view == lbr.build-history && viewItem == buildHistoryFile",
          "group": "inline"
        },
        {
          "command": "lbr.build-history.delete-date",
          "when": "view == lbr.build-history && viewItem == buildHistoryDate",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lbr-bar",
          "title": "Object Builder for i",
          "icon": "$(code)"
        }
      ]
    },
    "views": {
      "lbr-bar": [
        {
          "type": "webview",
          "id": "lbr.controller",
          "name": "LBR Build",
          "when": "lbr.contains_lbr_project && lbr.valid-config"
        },
        {
          "type": "tree",
          "id": "lbr.build-history",
          "name": "Build history",
          "when": "lbr.contains_lbr_project && lbr.valid-config"
        },
        {
          "type": "tree",
          "id": "lbr.source-filter",
          "name": "%Source filters%",
          "when": "lbr.contains_lbr_project && lbr.valid-config"
        },
        {
          "type": "webview",
          "id": "lbr.quick-settings",
          "name": "%Quick settings%",
          "when": "false && lbr.contains_lbr_project && lbr.valid-config"
        },
        {
          "type": "webview",
          "id": "lbr.deployment",
          "name": "%deployment%",
          "when": "1==2 && lbr.contains_lbr_project && lbr.valid-config"
        },
        {
          "type": "webview",
          "id": "lbr-welcome",
          "name": "%Welcome%",
          "when": "lbr.contains_lbr_project == false"
        },
        {
          "type": "webview",
          "id": "lbr-config-invalid",
          "name": "%Invalid config%",
          "when": "lbr.contains_lbr_project && !lbr.valid-config"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package --omit=dev",
    "compile": "node ./esbuild.js",
    "package": "NODE_ENV=production node ./esbuild.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "test": "node ./test/runTest.js",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.14.0",
    "@types/vscode": "^1.66.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "latest",
    "mocha": "^11.7.5",
    "ovsx": "latest",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "@emnapi/core": "^1.4.3",
    "@emnapi/runtime": "^1.4.3",
    "@tybys/wasm-util": "^0.9.0",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "deepmerge": "^4.3.1",
    "esbuild": "^0.25.10",
    "fs-extra": "^11.2.0",
    "hasha": "^6.0.0",
    "node-ssh": "^13.2.0",
    "nunjucks": "^3.2.4",
    "smol-toml": "^1.3.0",
    "wildcard-match": "latest",
    "winston": "latest",
    "winston-transport": "latest"
  }
}
